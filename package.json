{
    "name": "developer.beequip.nl",
    "version": "0.1.0",
    "description": "Developer portal for Beequip",
    "main": "index.js",
    "scripts": {
        "dev": "next --port 3001",
        "build": "next build",
        "start": "next start",
        "graphql:all": "yarn run graphql:download-schema && yarn run graphql:clean-generated-types && yarn run graphql:generate-types",
        "graphql:download-schema": "apollo service:download --config=apollo.config.js graphql.schema.json",
        "graphql:generate-types": "apollo client:codegen --target typescript --tagName gql --addTypename --outputFlat types/graphql",
        "graphql:clean-generated-types": "del './types/graphql/*'",
        "lint:check": "eslint --ext .js --ext .ts --ext .tsx .",
        "lint:fix": "yarn run lint:check --fix"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:BEEQUIP/developer.beequip.nl.git"
    },
    "author": "Beequip <info@beequip.nl>",
    "license": "MIT",
    "dependencies": {
        "@apollo/client": "^3.3.21",
        "@beequip/hexagon": "^0.9.5",
        "@mdx-js/react": "^1.6.22",
        "formik": "^2.1.4",
        "graphql": "^14",
        "graphql-tag": "^2.10.3",
        "isomorphic-unfetch": "^3.0.0",
        "next": "^11.0.1",
        "nextra": "^1.0.0-beta.2",
        "nextra-theme-docs": "^1.1.7",
        "normalize.css": "^8.0.1",
        "prism-react-renderer": "^1.2.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-router-dom": "^5.2.0",
        "styled-components": "^5.3.0",
        "styled-icons": "^9.0.1",
        "styled-map": "^3.3.0",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "yup": "^0.28.3"
    },
    "devDependencies": {
        "@beequip/eslint-config-react-typescript": "^0.3.0",
        "@types/react": "^16.9.16",
        "@types/styled-components": "^5.1.11",
        "@types/yup": "^0.26.34",
        "apollo": "^2.26.0",
        "babel-plugin-styled-components": "^1.13.2",
        "del-cli": "^3.0.0",
        "eslint": "^7.31.0",
        "prettier": "^2.3.2",
        "typescript": "^4.3.5"
    },
    "resolutions": {
        "graphql": "^14"
    }
}
